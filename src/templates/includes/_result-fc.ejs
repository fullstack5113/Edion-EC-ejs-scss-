<section class="fc-product-list">
    <div class="inner inner-large">
        <h2 class="section-heading">エアコン</h2>
            
        <div class="form-block">
            <form action="">
                <div class="form-block-sort">
                    <div class="form-block-sort-unit">
                        <label>在庫：</label>
                        <div class="form-block-select">
                            <select name="" id="">
                                <option value="">すべて</option>
                                <option value="">あり</option>
                                <option value="">店舗展示有</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-block-sort-unit is-pc">
                        <label>商品タイプ：</label>
                        <div class="form-block-select">
                            <select name="" id="">
                                <option value="">すべて</option>
                                <option value="">お買い得</option>
                                <option value="">人気の品</option>
                                <option value="">推奨品</option>
                                <option value="">新製品</option>
                                <option value="">予約商品</option>
                                <option value="">分割無金利対象商品</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-block-sort-unit is-pc">
                        <label>受取方法：</label>
                        <div class="form-block-select">
                            <select name="" id="">
                                <option value="">すべて</option>
                                <option value="">店舗受取可</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-block-sort-unit">
                        <div>
                            <label>並べ替え</label>
                            <div class="form-block-select">
                                <select name="" id="">
                                    <option value="">おすすめ順</option>
                                    <option value="">価格（安い順）</option>
                                    <option value="">価格（高い順）</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-block-sort-icons">
                            <button class="icon showgrid <% if ( result_style == 'grid') { %>is-active<% } %>"></button>
                            <button class="icon showlist <% if (result_style == 'list') { %>is-active<% } %>"></button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
        

